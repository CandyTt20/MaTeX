Cell[CellGroupData[{

Cell[TextData[{
 "In this case, specify the path to these executables manually using ",
 Cell[BoxData[
  ButtonBox["ConfigureMaTeX",
   BaseStyle->"Link",
   ButtonData->"paclet:MaTeX/ref/ConfigureMaTeX"]], "InlineFormula"],
 ". For example, on OS X with MacTeX the typical locations are the \
following:"
}], "Text",
 CellChangeTimes->{{3.68805240057126*^9, 3.688052458311243*^9}, {
  3.688106440465106*^9, 3.6881064686232557`*^9}, {3.688193281853887*^9, 
  3.688193289784389*^9}},
 CellID->1125038529],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ConfigureMaTeX", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
   "\"\<pdfLaTeX\>\"", "\[Rule]", "\"\</Library/TeX/texbin/pdflatex\>\""}], 
   ",", 
   RowBox[{"\"\<Ghostscript\>\"", "\[Rule]", "\"\</usr/local/bin/gs\>\""}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
 Evaluatable->False,
 CellChangeTimes->{{3.688051014610641*^9, 3.688051024643964*^9}, {
  3.688052365697544*^9, 3.68805237140901*^9}, {3.688106514918579*^9, 
  3.688106516972529*^9}},
 CellLabel->"In[2]:=",
 CellID->1768050685],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"CacheSize\"\>", "\[Rule]", "100"}], ",", 
   RowBox[{"\<\"WorkingDirectory\"\>", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"\<\"pdfLaTeX\"\>", 
    "\[Rule]", "\<\"/Library/TeX/texbin/pdflatex\"\>"}], ",", 
   RowBox[{"\<\"Ghostscript\"\>", "\[Rule]", "\<\"/usr/local/bin/gs\"\>"}]}], 
  "}"}]], "Output",
 CellChangeTimes->{
  3.688051030949843*^9, 3.688052372751333*^9, 3.6880524616745*^9, {
   3.68810659407615*^9, 3.688106619540339*^9}, 3.6881928614996243`*^9, 
   3.688192909017131*^9, 3.719909869323029*^9, 3.721375101791277*^9},
 CellLabel->"Out[2]=",
 CellID->1899709450]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
On Windows with MiKTeX, the paths may be similar to the following:\
\>", "Text",
 CellChangeTimes->{{3.764037044528747*^9, 3.7640370507220573`*^9}, {
   3.764037206215925*^9, 3.764037228518857*^9}, 3.7640373857065277`*^9, {
   3.764037710963854*^9, 3.7640377135364447`*^9}},
 CellID->1721985055],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ConfigureMaTeX", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
   "\"\<pdfLaTeX\>\"", "\[Rule]", 
    "\"\<C:\\\\Program Files\\\\MiKTeX \
2.9\\\\miktex\\\\bin\\\\x64\\\\pdflatex.exe\>\""}], ",", 
   RowBox[{
   "\"\<Ghostscript\>\"", "\[Rule]", 
    "\"\<C:\\\\Program Files\\\\gs\\\\gs9.27\\\\bin\\\\gswin64c.exe\>\""}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
 Evaluatable->False,
 CellChangeTimes->{{3.688051014610641*^9, 3.688051024643964*^9}, {
  3.688052365697544*^9, 3.68805237140901*^9}, {3.688106514918579*^9, 
  3.688106516972529*^9}, {3.764036965020771*^9, 3.7640369821183157`*^9}, {
  3.764037023162528*^9, 3.7640370378742943`*^9}},
 CellLabel->"In[2]:=",
 CellID->1654212509],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"CacheSize\"\>", "\[Rule]", "100"}], ",", 
   RowBox[{"\<\"WorkingDirectory\"\>", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"\<\"pdfLaTeX\"\>", 
    "\[Rule]", "\<\"C:\\\\Program Files\\\\MiKTeX 2.9\\\\miktex\\\\bin\\\\x64\
\\\\pdflatex.exe\"\>"}], ",", 
   RowBox[{"\<\"Ghostscript\"\>", 
    "\[Rule]", "\<\"C:\\\\Program \
Files\\\\gs\\\\gs9.27\\\\bin\\\\gswin64c.exe\"\>"}]}], "}"}]], "Output",
 CellLabel->"Out[2]=",
 CellID->1026344701]
}, Open  ]]
}, Open  ]],

Cell["\<\
Note that the paths must point to the executables, not to the directory \
containing them. Make sure to use correct capitalization.\
\>", "Text",
 CellChangeTimes->{{3.688052592162796*^9, 3.688052614930335*^9}, {
  3.688198854406661*^9, 3.688198861795597*^9}, {3.7639810021305027`*^9, 
  3.763981004078438*^9}},
 CellID->1411884930],

Cell[CellGroupData[{

Cell["The existing configuration can always be queried using", "Text",
 CellChangeTimes->{{3.6881933551703033`*^9, 3.68819336921373*^9}},
 CellID->1877514704],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ConfigureMaTeX", "[", "]"}]], "Input",
 CellChangeTimes->{{3.6881934016548758`*^9, 3.688193403494896*^9}},
 CellLabel->"In[3]:=",
 CellID->1947478611],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"CacheSize\"\>", "\[Rule]", "100"}], ",", 
   RowBox[{"\<\"WorkingDirectory\"\>", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"\<\"pdfLaTeX\"\>", 
    "\[Rule]", "\<\"/Library/TeX/texbin/pdflatex\"\>"}], ",", 
   RowBox[{"\<\"Ghostscript\"\>", "\[Rule]", "\<\"/usr/local/bin/gs\"\>"}]}], 
  "}"}]], "Output",
 CellChangeTimes->{3.688193404332549*^9, 3.719909880596238*^9, 
  3.721375104268857*^9},
 CellLabel->"Out[3]=",
 CellID->1207511615]
}, Open  ]]
}, Open  ]],

