 CellChangeTimes->{{3.688193440012474*^9, 3.688193440833027*^9}},
 CellID->120086388],

Cell[TextData[{
 "Any flavour of ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"T", 
     AdjustmentBox["E",
      BoxBaselineShift->0.5,
      BoxMargins->{{-0.3, 0}, {0, 0}}], "X"}],
    SingleLetterItalics->False], TraditionalForm]]],
 " that produces PDF output can be used, including pdfLaTeX, ",
 ButtonBox["XeLaTeX",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://tex.stackexchange.com/questions/3393/what-is-xetex-exactly-\
and-why-should-i-use-it"], None}],
 " and ",
 ButtonBox["LuaLaTeX",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://www.luatex.org/"], None}],
 ". Unlike pdfLaTeX, XeLaTeX and LuaLaTeX can load any installed system \
font."
}], "BulletedText",
 CellChangeTimes->{{3.6881066420799417`*^9, 3.6881066690580397`*^9}, {
  3.688193651570497*^9, 3.688193696298872*^9}, {3.68840810813687*^9, 
  3.688408150077697*^9}},
 CellID->874045736],

Cell[TextData[{
 "On Windows, use the Ghostscript executable which has a c suffix, i.e. ",
 StyleBox["gswin64c.exe", "InlineCode"],
 " instead of ",
 StyleBox["gswin64.exe", "InlineCode"],
 ". The typical Ghostscript installation location is ",
 StyleBox["C:\\Program Files\\gs\\.", "InlineCode"]
}], "BulletedText",
 CellChangeTimes->{{3.6881066420799417`*^9, 3.6881066690580397`*^9}, {
  3.688193459177033*^9, 3.688193596401609*^9}},
 CellID->1638087321]
}, Open  ]],

Cell[CellGroupData[{

